		{
			"name": "Create_user",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Check if the response status code is 201\", function () {",
							"    pm.response.to.have.status(201);",
							"});",
							"",
							"pm.test(\"Check if the response body contains the correct data\", function () {",
							"    let responseBody = pm.response.json();",
							"    pm.expect(responseBody.fullName.firstName).to.eql(\"Makan\");",
							"    pm.expect(responseBody.fullName.lastName).to.eql(\"Salehi\");",
							"    pm.expect(responseBody.email).to.eql(pm.collectionVariables.get(\"user_email\"));",
							"    pm.expect(responseBody.emailVerification).to.eql(false);",
							"    pm.expect(responseBody.dateOfBirth).to.eql(1753718796000);",
							"    pm.expect(responseBody.isAdmin).to.eql(false);",
							"    pm.expect(responseBody.profilePicture).to.not.be.empty;",
							"    pm.expect(responseBody.blockedUsers).to.eql([]);",
							"});"
						],
						"type": "text/javascript",
						"packages": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n    \"fullName\": {\n        \"firstName\": \"Makan\",\n        \"lastName\": \"Salehi\"\n    },\n    \"email\": \"{{user_email}}\",\n    \"emailVerification\": false,\n    \"dateOfBirth\": 1753718796000,\n    \"isAdmin\": false,\n    \"profilePicture\": \"{{$randomImageDataUri}}\",\n    \"password\": \"Omid1234@\"\n\n}",
